                    <Button x:Name="a02" Content="浏览" HorizontalAlignment="Left" Margin="624,195,0,0" VerticalAlignment="Top" Width="59" Click="a02_Click" FontFamily="Microsoft YaHei UI" FontWeight="Normal" Foreground="Black" Background="{x:Null}" FontSize="12" Height="28" Style="{DynamicResource ButtonStyle1}"/>
                    <Button x:Name="a03" Content="浏览" HorizontalAlignment="Left" Margin="624,226,0,0" VerticalAlignment="Top" Width="59" Click="a03_Click" FontFamily="Microsoft YaHei UI" FontWeight="Normal" Foreground="Black" Background="{x:Null}" FontSize="12" Height="28" Style="{DynamicResource ButtonStyle1}"/>
